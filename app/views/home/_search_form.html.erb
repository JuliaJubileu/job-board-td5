<%= form_with url: search_path, method: :get do %>
  <div class="input-group">
    <%= text_field_tag :q, 'Busque um cargo ou empresa', :class => "form-control form-control-lg text-wrap text-muted fs-6" %>
    <%= submit_tag 'Pesquisar', :class => "btn btn-outline-secondary btn-lg" %>
  </div>
<% end %>
